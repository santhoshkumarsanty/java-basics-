import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner o= new Scanner(System.in);
	    int a;
	    int num =o.nextInt();
	    int digit =0;
	    int n=num*num;
	    System.out.println(n);
	    while(n>0)
	    {
	        a=n%10;
	       digit=digit+a;
	       n=n/10;
	       
	    }
	    if (num==digit)
	    {
	        System.out.println("neon number");
	    }
	    else
	    {
	        System.out.println(" not neon number");
	    }
	    
		
	}
}
