import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner o=new Scanner(System.in);
	    int n=0,i;
	    int num =o.nextInt();
	    for ( i=1;i<num;i++) 
	    {
	        if(num%i==0)
	        {
	            n=n+i;
	            System.out.println(i);
	        }
	    }
	    if(num==n)
	    {
	        System.out.println(n+" is perfect number ");
	    }
	    else
	    {
	        System.out.println(n+" is not perfect number");
	    }

	}
}
